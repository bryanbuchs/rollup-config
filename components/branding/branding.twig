{{ attach_library('b2b/branding') }}

{%
  set config = [
    'branding',
  ]
%}

<hgroup{{attributes.addClass(classes,config)}}>
  {{ title_prefix }}
  <h1 class="site-name">
    <a href="{{ url }}">
      {{ site_name|raw }}
    </a>
  </h1>
  <p class="site-slogan">{{ site_slogan }}</p>
  {{ title_suffix }}
</hgroup>
